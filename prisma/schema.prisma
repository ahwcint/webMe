generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model User {
  id       Int    @id @default(autoincrement())
  userId   Int    @unique
  username String @unique
  password String

  @@index([userId])
}
